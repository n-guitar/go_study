# /etc/mysql/conf.d/docker.cnf
[mysqld]
character-set-server = utf8mb4
skip-host-cache
skip-name-resolve
user            = mysql
port            = 3306
basedir         = /usr
tmpdir          = /tmp
lc-messages-dir = /usr/share/mysql
explicit_defaults_for_timestamp.

[client]
default-character-set = utf8mb4
port            = 3306
socket          = /var/run/mysqld/mysqld.sock
user = root
password = root